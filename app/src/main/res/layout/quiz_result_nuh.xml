<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/quiz_background"
    tools:context="com.example.dell.alkitabanak.quiz_result_nuh">


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageViewResultNuh"
        android:scaleType="fitStart"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton10"
        android:onClick="onClickCobaLagi"
        android:layout_marginTop="10dp"
        android:background="@drawable/panah_coba_lagi"
        android:layout_below="@+id/imageViewResultNuh"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton11"
        android:layout_below="@+id/imageViewResultNuh"
        android:layout_centerHorizontal="true"
        android:onClick="onClickBacaUlang"
        android:layout_marginTop="10dp"
        android:background="@drawable/panah_baca_ulang"/>

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageButton12"
        android:onClick="onClickMenuUtama"
        android:layout_below="@+id/imageViewResultNuh"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="10dp"
        android:background="@drawable/panah_menu_utama"/>


</RelativeLayout>
